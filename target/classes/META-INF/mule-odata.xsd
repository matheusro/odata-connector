        <xs:attribute type="xs:string" use="optional" name="serviceUri">
          <xs:annotation>
            <xs:documentation>the URI of the target OData Service</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/odata" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="DeleteEntityType" substitutionGroup="mule:abstract-message-processor" name="delete-entity">
